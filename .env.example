# Database
DATABASE_URL="postgresql://user:password@host:5432/database?schema=public"

# JWT Secret (generate a random string)
JWT_SECRET="your-super-secret-jwt-key-change-this"

# Groq API (Required for AI playlist generation)
# Get from https://console.groq.com (free tier available)
GROQ_API_KEY="your-groq-api-key"

# AcoustID (Optional - for music recognition)
# Get free API key from https://acoustid.org/new-application
# Leave empty to use default demo key (rate-limited)
ACOUSTID_API_KEY=""

# Cloudinary (Optional - for profile image uploads)
# Get from https://cloudinary.com (free tier available)
CLOUDINARY_CLOUD_NAME="your-cloudinary-cloud-name"
CLOUDINARY_UPLOAD_PRESET="your-upload-preset"
CLOUDINARY_API_KEY="your-cloudinary-api-key"
CLOUDINARY_API_SECRET="your-cloudinary-api-secret"

# App URL
APP_URL="http://localhost:5173"




